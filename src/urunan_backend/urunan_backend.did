type Categories = record { icon : text; name : text };
type ExpenseDetails = record {
  tag : vec text;
  title : text;
  split_mode : SplitBillMode;
  currency : text;
  timestamp : nat64;
  amount : float64;
};
type SplitBillMode = variant { Portion; Evenly; PerPurchase };
service : {
  check_username : (text) -> (bool) query;
  get_cycles_balance : () -> (nat64) query;
  greet : (text) -> (text) query;
  new_expense : (ExpenseDetails, Categories) -> (nat);
}
